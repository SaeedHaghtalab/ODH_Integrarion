
record(stringin, "KG-GTA:ODH-PLC-01:ModVersionR") {
	field(DISP, "1")
	field(VAL,  "$(MODVERSION=N/A)")
	field(PINI, "YES")
}

#########################################################
########## EPICS <-> PLC connection management ##########
#########################################################
record(asyn, "KG-GTA:ODH-PLC-01:iAsyn") {
	field(DTYP,	"asynRecordDevice")
	field(PORT,	"$(PLCNAME)")
}
record(bi, "KG-GTA:ODH-PLC-01:ModbusConnectedR") {
	field(DESC,	"Shows if the MODBUS channel connected")
	field(INP,	"KG-GTA:ODH-PLC-01:iAsyn.CNCT CP")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
	field(FLNK,	"KG-GTA:ODH-PLC-01:CommsHashToPLCS")
}
record(bi, "KG-GTA:ODH-PLC-01:S7ConnectedR") {
	field(DESC,	"Shows if the S7 channel is connected")
	field(SCAN,	"I/O Intr")
	field(DTYP,	"S7plc stat")
	field(INP,	"@$(PLCNAME)")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
}
record(calcout, "KG-GTA:ODH-PLC-01:iCalcConn") {
	field(INPA,	"KG-GTA:ODH-PLC-01:S7ConnectedR CP")
	field(INPB,	"KG-GTA:ODH-PLC-01:ModbusConnectedR CP")
	field(CALC,	"A && B")
	field(OUT,	"KG-GTA:ODH-PLC-01:ConnectedR PP")
}
record(bi, "KG-GTA:ODH-PLC-01:ConnectedR") {
	field(DESC,	"Shows if the PLC is connected")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
}
record(bi, "KG-GTA:ODH-PLC-01:PLCHashCorrectR") {
	field(DESC,	"Shows if the comms hash is correct")
	field(ONAM,	"Correct")
	field(ZNAM,	"Incorrect")
	field(ZSV,      "MAJOR")
}
record(bi, "KG-GTA:ODH-PLC-01:AliveR") {
	alias("KG-GTA:ODH-PLC-01:CommsOK")
	field(DESC,	"Shows if the PLC is sending heartbeats")
	field(ONAM,	"Alive")
	field(ZNAM,	"Not responding")
	field(ZSV,      "MAJOR")
}
record(calcout, "KG-GTA:ODH-PLC-01:iCheckHash") {
	field(INPA,	"KG-GTA:ODH-PLC-01:iCommsHashToPLC")
	field(INPB,	"KG-GTA:ODH-PLC-01:CommsHashFromPLCR")
	field(CALC,	"A = B")
	field(OOPT,	"On Change")
	field(OUT,	"KG-GTA:ODH-PLC-01:PLCHashCorrectR PP")
}
record(bi, "KG-GTA:ODH-PLC-01:iOne") {
	field(DISP,	"1")
	field(PINI,	"YES")
	field(VAL,	"1")
}
record(bo, "KG-GTA:ODH-PLC-01:iGotHeartbeat") {
	field(DOL,	"KG-GTA:ODH-PLC-01:iOne")
	field(OMSL,	"closed_loop")
	field(OUT,	"KG-GTA:ODH-PLC-01:iKickAlive PP")
}
record(bo, "KG-GTA:ODH-PLC-01:iKickAlive") {
	field(HIGH,	"5")
	field(OUT,	"KG-GTA:ODH-PLC-01:AliveR PP")
}

########################################################
########## EPICS -> PLC comms management data ##########
########################################################
record(ao, "KG-GTA:ODH-PLC-01:iCommsHashToPLC") {
	field(DISP,	"1")
	field(PINI,	"YES")
	field(VAL,	"733470530")
}
record(ao, "KG-GTA:ODH-PLC-01:CommsHashToPLCS") {
	field(DESC,	"Sends comms hash to PLC")
	field(SCAN,	"1 second")
	field(DTYP,	"asynInt32")
	field(OUT,	"@asyn($(PLCNAME)write, 0, 100)INT32_BE")
	field(OMSL,	"closed_loop")
	field(DOL,	"KG-GTA:ODH-PLC-01:iCommsHashToPLC")
	field(DISV,	"0")
	field(SDIS,	"KG-GTA:ODH-PLC-01:ModbusConnectedR")
}
record(calc, "KG-GTA:ODH-PLC-01:iHeartbeatToPLC") {
	field(SCAN,	"1 second")
	field(INPA,	"KG-GTA:ODH-PLC-01:iHeartbeatToPLC.VAL")
	field(CALC,	"(A >= 32000)? 0 : A + 1")
	field(FLNK,	"KG-GTA:ODH-PLC-01:HeartbeatToPLCS")
	field(DISV,	"0")
	field(SDIS,	"KG-GTA:ODH-PLC-01:ModbusConnectedR")
}
record(ao, "KG-GTA:ODH-PLC-01:HeartbeatToPLCS") {
	field(DESC,	"Sends heartbeat to PLC")
	field(DTYP,	"asynInt32")
	field(OUT,	"@asyn($(PLCNAME)write, 2, 100)")
	field(OMSL,	"closed_loop")
	field(DOL,	"KG-GTA:ODH-PLC-01:iHeartbeatToPLC.VAL")
	field(OIF,	"Full")
	field(DRVL,	"0")
	field(DRVH,	"32000")
	field(DISV,	"0")
	field(SDIS,	"KG-GTA:ODH-PLC-01:ModbusConnectedR")
}

########################################################
########## PLC -> EPICS comms management data ##########
########################################################
record(ai, "KG-GTA:ODH-PLC-01:CommsHashFromPLCR") {
	field(DESC,	"Comms hash from PLC")
	field(SCAN,	"I/O Intr")
	field(DTYP,	"S7plc")
	field(INP,	"@$(PLCNAME)/0 T=INT32")
	field(FLNK,	"KG-GTA:ODH-PLC-01:iCheckHash")
}
record(ai, "KG-GTA:ODH-PLC-01:HeartbeatFromPLCR") {
	field(DESC,	"Heartbeat from PLC")
	field(SCAN,	"I/O Intr")
	field(DTYP,	"S7plc")
	field(INP,	"@$(PLCNAME)/4 T=INT16")
	field(FLNK,	"KG-GTA:ODH-PLC-01:iGotHeartbeat")
	field(DISS,	"INVALID")
	field(DISV,	"0")
	field(SDIS,	"KG-GTA:ODH-PLC-01:PLCHashCorrectR")
}



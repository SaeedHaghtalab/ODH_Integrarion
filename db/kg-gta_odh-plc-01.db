record(asyn, "KG-GTA:ODH-PLC-01:iAsyn")
{
	field(DTYP,	"asynRecordDevice")
	field(PORT,	"$(PLCNAME)")
}
record(bi, "KG-GTA:ODH-PLC-01:ModbusConnectedR")
{
	field(DESC,	"Shows if the MODBUS channel connected")
	field(INP,	"KG-GTA:ODH-PLC-01:iAsyn.CNCT CP")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
	field(FLNK,	"KG-GTA:ODH-PLC-01:CommsHashToPLCS")
}
record(bi, "KG-GTA:ODH-PLC-01:S7ConnectedR")
{
	field(DESC,	"Shows if the S7 channel is connected")
	field(SCAN,	"I/O Intr")
	field(DTYP,	"S7plc stat")
	field(INP,	"@$(PLCNAME)")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
}
record(stringin, "KG-GTA:ODH-PLC-01:PLCAddr-RB")
{
# We assume S7 and Modbus address are the same (as they should be)
	field(DESC,	"Address of the PLC")
}
record(scalcout, "KG-GTA:ODH-PLC-01:iPLCAddr-RB")
{
	field(DESC,	"Strip port number of host:port")
	field(INAA,	"KG-GTA:ODH-PLC-01:S7Addr-RB CP")
	field(CALC,	"AA[0,':']")
	field(OUT,	"KG-GTA:ODH-PLC-01:PLCAddr-RB PP")
}
record(stringout, "KG-GTA:ODH-PLC-01:PLCAddrS")
{
	field(DESC,	"Address of the PLC")
	field(FLNK,	"KG-GTA:ODH-PLC-01:iSetPLCAddrS")
}
record(fanout, "KG-GTA:ODH-PLC-01:iSetPLCAddrS")
{
	field(LNK1,	"KG-GTA:ODH-PLC-01:iCalcS7AddrS")
	field(LNK2,	"KG-GTA:ODH-PLC-01:iCalcModbusAddrS")
}
record(scalcout, "KG-GTA:ODH-PLC-01:iCalcS7AddrS")
{
	field(INAA,	"KG-GTA:ODH-PLC-01:PLCAddrS")
	field(CALC,	"AA + ':' + '$(S7_PORT)'")
	field(OUT,	"KG-GTA:ODH-PLC-01:iS7AddrS PP")
}
record(scalcout, "KG-GTA:ODH-PLC-01:iCalcModbusAddrS")
{
	field(INAA,	"KG-GTA:ODH-PLC-01:PLCAddrS")
	field(CALC,	"AA + ':' + '$(MODBUS_PORT)'")
	field(OUT,	"KG-GTA:ODH-PLC-01:iAsyn.HOSTINFO PP")
}
record(stringin, "KG-GTA:ODH-PLC-01:ModbusAddr-RB")
{
	field(DESC,	"Address of the PLC")
	field(INP,	"KG-GTA:ODH-PLC-01:iAsyn.HOSTINFO CP")
}
record(stringin, "KG-GTA:ODH-PLC-01:S7Addr-RB")
{
	field(DESC,	"Address of the PLC")
	field(INP,	"KG-GTA:ODH-PLC-01:iS7AddrS CP")
}
record(stringout, "KG-GTA:ODH-PLC-01:iS7AddrS")
{
	field(DESC,	"Set address of the PLC")
	field(DTYP,	"S7plc addr")
	field(OUT,	"@$(PLCNAME)")
	field(DISP,	"1")
}
record(calcout, "KG-GTA:ODH-PLC-01:iCalcConn")
{
	field(INPA,	"KG-GTA:ODH-PLC-01:S7ConnectedR CP")
	field(INPB,	"KG-GTA:ODH-PLC-01:ModbusConnectedR CP")
	field(CALC,	"A && B")
	field(OUT,	"KG-GTA:ODH-PLC-01:ConnectedR PP")
}
record(bi, "KG-GTA:ODH-PLC-01:ConnectedR")
{
	field(DESC,	"Shows if the PLC is connected")
	field(ONAM,	"Connected")
	field(ZNAM,	"Disconnected")
	field(ZSV,      "MAJOR")
}
record(bi, "KG-GTA:ODH-PLC-01:PLCHashCorrectR")
{
	field(DESC,	"Shows if the comms hash is correct")
	field(ONAM,	"Correct")
	field(ZNAM,	"Incorrect")
	field(ZSV,      "MAJOR")
}
record(bi, "KG-GTA:ODH-PLC-01:AliveR")
{
	alias("KG-GTA:ODH-PLC-01:CommsOK")
	field(DESC,	"Shows if the PLC is sending heartbeats")
	field(ONAM,	"Alive")
	field(ZNAM,	"Not responding")
	field(ZSV,      "MAJOR")
}
record(calcout, "KG-GTA:ODH-PLC-01:iCheckHash")
{
	field(INPA,	"KG-GTA:ODH-PLC-01:iCommsHashToPLC")
	field(INPB,	"KG-GTA:ODH-PLC-01:CommsHashFromPLCR")
	field(INPC,	"KG-GTA:ODH-PLC-01:CommsHashFromPLCR.STAT")
	field(CALC,	"A == B && C == 0")
	field(OOPT,	"On Change")
	field(OUT,	"KG-GTA:ODH-PLC-01:PLCHashCorrectR PP")
}
record(bo, "KG-GTA:ODH-PLC-01:iGotHeartbeat")
{
	field(DOL,	"KG-GTA:ODH-PLC-01:iOne")
	field(OMSL,	"closed_loop")
	field(OUT,	"KG-GTA:ODH-PLC-01:iKickAlive PP")
}
record(bo, "KG-GTA:ODH-PLC-01:iKickAlive")
{
	field(HIGH,	"5")
	field(OUT,	"KG-GTA:ODH-PLC-01:AliveR PP")
}

